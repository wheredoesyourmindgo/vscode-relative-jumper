{
  "name": "vscode-relativity",
  "displayName": "Relativity",
  "description": "A Visual Studio Code extension that allows you to jump and select lines relatively.",
  "version": "0.2.0",
  "license": "MIT",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "icon": "resources/icon.webp",
  "keywords": [
    "jump",
    "lines",
    "navigate",
    "code",
    "select",
    "vscode",
    "extension"
  ],
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "relativity.jumpUp",
        "title": "Jump Up"
      },
      {
        "command": "relativity.jumpDown",
        "title": "Jump Down"
      },
      {
        "command": "relativity.selectUp",
        "title": "Select Up"
      },
      {
        "command": "relativity.selectDown",
        "title": "Select Down"
      }
    ],
    "keybindings": [
      {
        "command": "relativity.jumpUp",
        "key": "cmd+9",
        "when": "editorTextFocus"
      },
      {
        "command": "relativity.jumpDown",
        "key": "cmd+8",
        "when": "editorTextFocus"
      },
      {
        "command": "relativity.selectUp",
        "key": "cmd+shift+9",
        "when": "editorTextFocus"
      },
      {
        "command": "relativity.selectDown",
        "key": "cmd+shift+8",
        "when": "editorTextFocus"
      }
    ]
  },
  "publisher": "wheredoesyourmindgo",
  "repository": {
    "type": "git",
    "url": "https://github.com/wheredoesyourmindgo/vscode-relativity"
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
